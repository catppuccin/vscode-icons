{
  "publisher": "Catppuccin",
  "name": "catppuccin-vsc-icons",
  "displayName": "Catppuccin Icons for VSCode",
  "type": "commonjs",
  "version": "0.34.0",
  "private": true,
  "packageManager": "pnpm@8.14.3",
  "description": "ü¶ä Soothing pastel icon theme for VSCode",
  "author": "prazdevs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/catppuccin/vscode-icons"
  },
  "keywords": [
    "catppuccin",
    "theme",
    "fancy",
    "pastel-colors",
    "vscode",
    "vscode-theme",
    "vscode-icons",
    "vscode-extension"
  ],
  "categories": [
    "Themes"
  ],
  "main": "dist/index.js",
  "icon": "icon.png",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "engines": {
    "vscode": "^1.83.0",
    "node": ">=20.0.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "iconThemes": [
      {
        "id": "catppuccin-mocha",
        "label": "Catppuccin Mocha",
        "path": "./themes/mocha.json"
      },
      {
        "id": "catppuccin-latte",
        "label": "Catppuccin Latte",
        "path": "./themes/latte.json"
      },
      {
        "id": "catppuccin-frappe",
        "label": "Catppuccin Frapp√©",
        "path": "./themes/frappe.json"
      },
      {
        "id": "catppuccin-macchiato",
        "label": "Catppuccin Macchiato",
        "path": "./themes/macchiato.json"
      }
    ]
  },
  "scripts": {
    "build": "tsx scripts/build.ts",
    "icons:generate": "tsx scripts/generate.ts",
    "icons:optimize": "tsx scripts/optimize.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@catppuccin/palette": "^1.0.3",
    "@iconify/tools": "^4.0.0",
    "@types/node": "^20.10.8",
    "@types/vscode": "^1.83.0",
    "@vscode/vsce": "^2.22.0",
    "eslint": "^8.56.0",
    "magic-regexp": "^0.7.0",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "type-flag": "^3.0.0",
    "typescript": "^5.3.3"
  }
}
