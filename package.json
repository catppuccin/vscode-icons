{
  "publisher": "Catppuccin",
  "name": "@catppuccin/code-icons",
  "type": "module",
  "version": "1.21.0",
  "private": true,
  "packageManager": "pnpm@10.10.0",
  "fontVersion": "1.0",
  "description": "ðŸ¦Š Soothing Code Icons for the high spirited!",
  "fontDescription": "Soothing Code Icons for the high spirited!",
  "author": "prazdevs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/catppuccin/vscode-icons"
  },
  "bugs": {
    "url": "https://github.com/catppuccin/vscode-icons/issues"
  },
  "sponsor": {
    "url": "https://opencollective.com/catppuccin"
  },
  "keywords": [
    "catppuccin",
    "theme",
    "fancy",
    "pastel-colors",
    "code-icons"
  ],
  "categories": [
    "Themes"
  ],
  "scripts": {
    "catwalk": "tsx scripts/catwalk.ts",
    "icons:generate": "tsx scripts/icons/generate.ts",
    "icons:optimize": "tsx scripts/icons/optimize.ts",
    "icons:preview": "tsx scripts/icons/preview.ts",
    "build": "pnpm run clean && pnpm run icons:optimize && tsx scripts/build.ts",
    "icons": "pnpm run --sequential '/^icons:.*/'",
    "previews": "pnpm dlx puppeteer browsers install chrome && pnpm run icons:preview && pnpm run catwalk",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "changelogen --release",
    "typecheck": "tsc",
    "clean": "tsx scripts/reset.ts",
    "genmap": "tsx scripts/genmap.ts"
  },
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@antfu/eslint-config": "^4.13.2",
    "@catppuccin/palette": "^1.7.1",
    "@iconify/tools": "^4.1.2",
    "@twbs/fantasticon": "^3.1.0",
    "@types/node": "^22.15.21",
    "@types/svg-sprite": "^0.0.39",
    "changelogen": "^0.6.1",
    "consola": "^3.4.2",
    "eslint": "^9.27.0",
    "execa": "^9.5.3",
    "lint-staged": "^15.5.2",
    "lookpath": "^1.2.3",
    "puppeteer": "^24.9.0",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.0",
    "svg-sprite": "^2.0.4",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "woff2otf": "^1.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "puppeteer",
      "simple-git-hooks",
      "ttf2woff2"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
